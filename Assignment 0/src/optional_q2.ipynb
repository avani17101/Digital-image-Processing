    "import cv2\n",
    "path = '../images/'\n",
    "videoCaptureObject = cv2.VideoCapture(0)\n",
    "result = True\n",
    "while(result):\n",
    "    ret,frame = videoCaptureObject.read()\n",
    "    cv2.imwrite(path+\"webcampic.jpg\",frame)\n",
    "    result = False\n",
    "videoCaptureObject.release()\n"
   ]
